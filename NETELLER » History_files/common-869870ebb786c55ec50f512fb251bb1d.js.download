        // Decide if we want to enable/disable submit
        if (errorCount > 0) {
            this.disableSubmitBtn();
        } else {
            this.enableSubmitBtn();
        }

        // If there are validation errors, return false to prevent form submit
        if (typeof event === 'object' && errorCount === 0) {
            this.showButtonSpinner();
            this.disableSubmitBtn();

            return true;
        } else if (errorCount === 0) {
            return true;
        } else {
            if(typeof event === 'object') {
                event.preventDefault();
            }

            return false;
        }
    };

