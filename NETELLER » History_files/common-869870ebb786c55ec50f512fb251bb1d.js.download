function updateCryptoBalance() {
	if (getBalanceUrl) {
		$.ajax({
			type : "GET",
			url : getBalanceUrl,
			dataType : "json",
			async : true,
			success : function(data) {
				$("#balance-menu-total").text(data.totalAssets);
				$("#balance-spinner").slideUp();
				if (data.cryptoBalanceList && data.cryptoBalanceList.length > 0) {
					$.each(data.cryptoBalanceList, function(i, item) {
						var rowId = 'crypto-balance-row-' + item.currency;
						var rowSelector = '#' + rowId;
						var oldRow = $(rowSelector);
						var isNewRow = oldRow.length === 0;
						var cryptoRow;
						if (!isNewRow) {
							cryptoRow = oldRow;
						} else {
							cryptoRow = $('#crypto-balance-row-template').clone();
							cryptoRow.attr('id', rowId);

							var logoImg = $(cryptoRow).find('#crypto-logo-img');
							var logoUrl = logoImg.data("resource");
							logoUrl = logoUrl.replace('.svg', item.currency + '.svg');
							logoImg.attr('src', logoUrl);

							cryptoRow.find('#crypto-currency-code').text(item.currency);
							cryptoRow.find('#crypto-currency-name').text(item.displayName);
						}
						cryptoRow.find('#crypto-currency-balance').text(item.cryptoBalance);
						cryptoRow.find('#fiat-currency-balance').text(item.fiatBalance);

						if (isNewRow) {
							$('#balance-total-line').before(cryptoRow);
							cryptoRow.slideDown();
						}
					});
				} else {
					$('#no-crypto-balance').show();
				}
			},
			error : function(req, status, error) {
				$("#balance-spinner").slideUp();
				$('#crypto-balance-error').show();
				if (req.status === 401) {
					window.location.reload();
				}
			}
		});
	}
}

