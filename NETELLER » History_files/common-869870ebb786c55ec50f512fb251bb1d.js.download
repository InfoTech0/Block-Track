var dataSlider = function(self) {

    //$(self).closest('.content').addClass('active');

    var $this = $(self);
    var windowSize = $this.attr('data-slider');
    var validSize = isWindowSizeValid(windowSize);
    if (!validSize) {return;}

    var $slides = $this.find('> [data-slide]');
    var $sliderControlsContainer = $('<ul class="slider-controls">');
    var width = $this.width();

    var setMaxHeight = function() {
        validSize = isWindowSizeValid(windowSize);

        if (!validSize) {
            return;
        }

        var maxHeight = 0;

        $slides.each(function(index) {
            var $slide = $(this);
            var slideHeight = $slide.height();

            if (slideHeight > maxHeight) {
                maxHeight = slideHeight;
            }

            $slide.css('left', width * index);
        });

        // @TODO: Find a better fix
        if (maxHeight === 0) {
            maxHeight = 504;
        }

        $this.height(maxHeight);
    };

    $slides.each(function() {
        $sliderControlsContainer.append('<li>');
    });

    var $sliderControls = $sliderControlsContainer.find('li');
    $sliderControls.first().addClass('active');
    $this.parent().append($sliderControlsContainer);

    $sliderControls.on('click', function() {
        var selectedIndex = $sliderControls.index(this);
        var activeSlide = Math.round(Math.abs($slides.position().left / width));
        var left = null;

        if (selectedIndex == activeSlide) {
            return;
        }

        if (selectedIndex > activeSlide) {
            left = '-=' + (width * (selectedIndex - activeSlide));
        } else {
            left = '+=' + (width * (activeSlide - selectedIndex));
        }

        $slides.each(function() {
            $(this).stop().animate({
                'left': left
            }, 250);
        });

        $sliderControls.removeClass('active');
        $(this).addClass('active');
    });

    $this.on('swipeleft', function() {
        var activeSlide = Math.round(Math.abs($slides.position().left / width));

        if (activeSlide > $slides.length) {
            return;
        }

        $sliderControls.eq(activeSlide + 1).trigger('click');
    });

    $this.on('swiperight', function() {
        var activeSlide = Math.round(Math.abs($slides.position().left / width));

        if (activeSlide === 0) {
            return;
        }

        $sliderControls.eq(activeSlide - 1).trigger('click');
    });

    $(window).on('resize', function() {
        width = $this.width();
        setMaxHeight();
    }).trigger('resize');

    $(this).closest('.content').removeClass('active');
}

