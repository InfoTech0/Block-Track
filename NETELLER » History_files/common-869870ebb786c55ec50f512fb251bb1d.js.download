// menu located in the header with the 'selected' attribute and set the textContent to an empty string fixing the text which sits on top of the desired layout.

(function edgeFix() {
    // Define needed variables (Options, browser, and undefined Selected variable)
    // let select = document.body.querySelector('#form-language-language' || '#lang');
    var options = document.body.querySelectorAll('#form-language-language > option' || '#lang > option');
