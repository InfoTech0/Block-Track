function checkIfAnyRequiredFieldMissing(form) {
    var hasMissingField = false;
    form.find('[required]').each(function () {
        if ($(this).val() === '') {
            hasMissingField = true;
            return false;
        }
    });
    return hasMissingField;
}

function validateFormUsingAbide(context, buttonSearchQuery) {
    disableSubmitButtonIfAnyRequiredFieldMissing(context, buttonSearchQuery);
    context
        // field element is invalid
        .on("invalid.zf.abide", function (ev, elem) {
            var $form = elem.closest('form');
            $form.find(buttonSearchQuery).prop('disabled', true);
        })
        // field element is valid
        .on("valid.zf.abide", function (ev, elem) {
            var $form = elem.closest('form');
            if ($form.find('.is-invalid-input')[0]) {
                $form.find(buttonSearchQuery).prop('disabled', true);
            } else if (checkIfAnyRequiredFieldMissing($form)) {
                $form.find(buttonSearchQuery).prop('disabled', true);
            } else {
                $form.find(buttonSearchQuery).prop('disabled', false);
            }
        });
}

