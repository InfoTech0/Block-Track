Neteller.Notifications = Neteller.Base.extend({
    notificationTypeClasses: {
        success: '',
        error  : 'error',
        alert  : 'secondary'
    },

    /**
     *
     * @param string  elementId    ID of the element we want to add notification to
     * @param string  type         Type of notification - success/error/alert
     * @param boolean showCloseBtn Do we want to show the close button?
     */
    init: function(text, elementId, type, showCloseBtn, autoHide)
    {
        var $container = $('#' + elementId);

        // Set default notification type to error
        if (typeof type == 'undefined') {
            type = 'error';
        }

        // Create new notification element
        var $notification = $('<div></div>').addClass(this.notificationTypeClasses[type] + '  alert-box').text(text).hide();
        this.addEvent($container, 'remove-notification', 'removeNotification'); // Bind event to remove notifications

        // Create close button, if we want to show one (defaults to true)
        if (showCloseBtn || typeof showCloseBtn == 'undefined') {
            var $closeButton = $('<a href="#" class="close">&times;</a>');

            this.addEvent($closeButton, 'click', 'onClickCloseNotification'); // Bind event to close notification upon clicking close button

            $notification.append($closeButton);
        }

        // If autohide is enabled, let's auto hide this notification
        if (autoHide) {
            var self = this;

            setTimeout(function() {
                self.closeNotification($notification);
            }, 5000);
        }

        // Append error to the container and display it
        $container.append($notification);
        $notification.slideDown();
    },

    onClickCloseNotification: function(event)
    {
        event.preventDefault();

        this.closeNotification($(event.target).closest('.alert-box'));
    },

    removeNotification: function(event)
    {
        this.closeNotification($(event.target));
    },

    closeNotification: function($notification)
    {
        $notification.slideUp(300, function() {
            $(this).remove();
        });
    }
});

