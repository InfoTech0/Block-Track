          // If the error field does not exist, then let's create it
          if (error === null) {
            error = document.createElement('small');
            error.className = 'error';
            self._getInputWrapper(field).appendChild(error);

            var info = self._getInputWrapper(field).querySelector('.info');
            if (info) { self._getInputWrapper(field).appendChild(info); }
