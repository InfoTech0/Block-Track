function isWindowSizeValid(size)
{
    switch (size) {
        case 'small-up':
            return true;
        case 'small-only':
            if ($window.width() >= smallWindow.min && $window.width() <= smallWindow.max) {
                return true;
            }
            break;
        case 'medium-up':
            if ($window.width() >= mediumWindow.min) {
                return true;
            }
            break;
    }

    return false;
}

